<p id="notice"><%= notice %></p>

<h2 class="todo_list_title"><%= @project_list.title %></h2>
<p class="todo_list_sub_title"><%= @project_list.description %></p>

<div id="todo_items_wrapper">
  <%= render @project_list.todo_items %>
  <div id="form">
    <%= render "todo_items/form" %>
  </div>
</div>

<div class="links">
  <%= link_to 'Edit', edit_project_list_path(@project_list) %> |
  <%= link_to 'Delete', project_list_path(@project_list), method: :delete, data: {confirm: "Are you sure?" } %> |
  <%= link_to 'Back', root_path %>
</div>
